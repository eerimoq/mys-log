from . import Logger

def main(args: [string]):
    if args.length() != 2:
        raise Exception(f"Usage: {args[0]} <message>")

    logger = Logger("log-main")

    logger.error("Message: {}", args[1])
    logger.warning("Message: {}", args[1])
    logger.info("Message: {}", args[1])
    logger.debug("Message: {}", args[1])
